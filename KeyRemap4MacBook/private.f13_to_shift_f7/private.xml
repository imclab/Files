<?xml version="1.0"?>
<root>

  <item>
    <name>Change f13 to shift-f7</name>
    <identifier>private.f13_to_shift_f7</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::F13, ModifierFlag::NONE,
      KeyCode::F7, ModifierFlag::SHIFT_L
    </autogen>
  </item>

  <item>
    <name>Change shift-f7 to f13</name>
    <identifier>private.shift_f7_to_f13</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::F7, MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_SHIFT,
      KeyCode::F13,
    </autogen>

    <autogen>
      __ConsumerToKey__
      ConsumerKeyCode::VOLUME_MUTE, MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_SHIFT,
      KeyCode::F13,
    </autogen>
  </item>

</root>
